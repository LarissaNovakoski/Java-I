  public static void main (String[] args)
    {
        int nota = 13;
        String selecao = "A sua nota foi ";
        if (nota <=10 && nota>=0)
        {
             switch (nota)
            {
                case 10:
                    selecao += "Excelente";
                case 9:
                    selecao += "Ótima";
                case 8:
                    selecao += "Boa";
                case 7:
                    selecao += "Satisfatoria";
                break;
                default:
                    selecao += "Regular";
            }
         System.out.println (selecao);
        }
        else
        {
            System.out.println ("Você digitou um número inválido!");
        }                   
